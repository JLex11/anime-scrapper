{
  "name": "anime-scrapper",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "node:dev": "ts-node-dev --project ./tsconfig.json api/devIndex.ts",
    "dev": "bun --watch api/devIndex.ts",
    "dev:file": "ts-node-dev",
    "build": "bun build --target=bun --entrypoints api/index.ts --outdir ./public/api",
    "build:dev": "bun build --target=bun --entrypoints api/devIndex.ts --outdir ./public/api",
    "node:start": "node ./public/api/index.js",
    "start": "bun run ./public/api/index.js",
    "node:start:dev": "node ./public/api/devIndex.js",
    "start:dev": "bun run api/devIndex.ts",
    "dev:workers": "bun --watch src/scripts/workers.ts",
    "start:workers": "bun run src/scripts/workers.ts",
    "lint": "bunx biome lint --write && bunx biome format --write",
    "analyze": "bun build --target=bun --entrypoints api/index.ts --analyze",
    "clean-cache": "rm -rf node_modules/.cache",
    "build:optimized": "NODE_ENV=production bun build --target=bun --minify --entrypoints api/index.ts --outdir ./public/api",
    "docs:build": "echo 'API Documentation is ready for deployment'"
  },
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/credential-provider-ini": "3.299.0",
    "@supabase/supabase-js": "2.49.4",
    "@types/amqplib": "^0.10.7",
    "@types/compression": "^1.7.5",
    "aws-sdk": "2.1692.0",
    "compression": "^1.8.0",
    "cors": "2.8.5",
    "express": "4.21.2",
    "express-rate-limit": "^7.5.0",
    "jsdom": "^26.1.0",
    "node-cache": "5.1.2",
    "sharp": "^0.33.5",
    "supabase": "^2.22.12",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "3.775.0",
    "@biomejs/biome": "1.9.4",
    "@types/bun": "^1.2.12",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.1",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.15.3",
    "ts-node-dev": "2.0.0",
    "ts-standard": "^12.0.2",
    "typescript": "5.8.3"
  },
  "ts-standard": {
    "ignore": [
      "public"
    ]
  },
  "private": true
}
